{
	"name": "~TODO~",
	"version": "0.0.1",
	"scripts": {
		"dev:only": "svelte-kit dev",
		"build:only": "svelte-kit build",
		"preview": "svelte-kit preview",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development tailwindcss -i src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production tailwindcss -i src/styles/tailwind.css -o src/styles/tailwind-output.css --minify",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@paypal/paypal-js": "^4.2.1",
		"@stripe/stripe-js": "^1.21.1",
		"@sveltejs/gestures": "^0.0.1",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.3.7",
		"concurrently": "^6.4.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.12",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"luxon": "^2.2.0",
		"postcss": "^8.4.4",
		"postcss-import": "^14.0.2",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.2",
		"svelte-carousel": "^1.0.16",
		"svelte-check": "^2.2.10",
		"svelte-loading-spinners": "^0.1.7",
		"svelte-media": "^0.1.5",
		"svelte-preprocess": "^4.9.8",
		"svelte-range-slider-pips": "^2.0.3",
		"svelte-star-rating": "^1.3.0",
		"svelte-stripe-js": "0.0.8",
		"svelte-tabs": "^1.1.0",
		"tailwindcss": "^2.2.19",
		"tslib": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.33"
	}
}
